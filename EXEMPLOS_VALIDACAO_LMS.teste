Exemplo 8: -----------------------------------------------------------------------------------------------
(*TESTE - valido com duas procedures*)
PROGRAM TESTEPROC;
	(*Proc A*)
	PROCEDURE pa(idd : INTEGER);
		VAR 
		    X, Y, Z : INTEGER;
	BEGIN
		X := X * Y;
	END;

	(*Proc B com os mesmos dados de A*)
	PROCEDURE pb(idd : INTEGER);
		VAR 
		    X, Y, Z : INTEGER;
	BEGIN
		X := Z *Z;
	END;


BEGIN
	call pa(10 + 2);
	call pb(5);
END.

